<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="timeControl" match-name="dia" />
	<table-filter match-name="atividade" match-catalog="timeControl"></table-filter>
	<table-filter match-name="atividade_dia"
		match-catalog="timeControl">
	</table-filter>
	<table-filter match-name="usuario" match-catalog="timeControl"></table-filter>
	<table catalog="timeControl" name="usuario"></table>
	<table catalog="timeControl" name="atividade"></table>
	<table catalog="timeControl" name="dia"></table>
	<table catalog="timeControl" name="atividade_dia"></table>
</hibernate-reverse-engineering>